<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- <bean class="org.springframework.beans.factory.annotation.RequiredAnnotationBeanPostProcessor"/> -->

	<bean id="jnjLaSyncCatalogService" class="com.jnj.la.masterstore.services.JnJLaSyncCatalogService">
		<property name="cronJobService" ref="cronJobService" />
		<property name="modelService" ref="modelService" />
	</bean>

	<bean name="jnjlamasterAcceleratorCoreSystemSetup" class="com.jnj.la.masterstore.setup.JnjlamasterstoreSystemSetup"  parent="abstractCoreSystemSetup">
		<property name="commonI18NService" ref="commonI18NService"/>
		<property name="defaultSetupSolrIndexerService" ref="defaultSetupSolrIndexerService" />
		<property name="syncCatalogService" ref="jnjLaSyncCatalogService" />
	</bean>
</beans>
