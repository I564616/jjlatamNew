<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<alias name="jnjLASSOUserService" alias="customSSOUserService"/>
	<bean id="jnjLASSOUserService" class="com.jnj.la.singlesignon.JnjLACustomSSOService" parent="abstractService">
		<property name="modelService" ref="modelService"/>
		<property name="userService" ref="userService"/>
		<property name="commonI18NService" ref="commonI18NService"/>
	</bean>

	<alias name="jnjLaRedirectionControllerBase" alias="redirectionControllerBase"/>
	<bean id="jnjLaRedirectionControllerBase" class="com.jnj.la.singlesignon.JnjLaRedirectionControllerBase" />

	<alias name="DefaultJnjLaBackofficeSSOService" alias="jnjLaBackofficeSSOService"/>
	<bean id="DefaultJnjLaBackofficeSSOService" class="com.jnj.la.sso.service.JnJLABackofficeSSOService"/>
</beans>