<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--suppress XmlDefaultAttributeValue -->
<config xmlns="http://www.hybris.com/cockpit/config">

    <context type="JnjLAEmailMatrix" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="previousStatus" selected="true" operator="equals"/>
                <field name="currentStatus" selected="true" operator="equals"/>
                <field name="sendEmail"/>
            </field-list>
        </advanced-search>
    </context>

    <context type="JnjLAOrderPermutationMatrix" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="hasCreated" />
                <field name="hasUnderAnalysis" />
                <field name="hasBeingProcessed" />
                <field name="hasInPicking" />
                <field name="hasPartiallyInvoiced" />
                <field name="hasInvoiced" />
                <field name="hasPartiallyShipped" />
                <field name="hasShipped" />
                <field name="hasPartiallyDelivered" />
                <field name="hasDelivered" />
                <field name="hasBackordered" />
                <field name="hasCancelled" />
                <field name="status" />
            </field-list>
        </advanced-search>
    </context>

    <context type="Order" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="sapOrderNumber" selected="true" operator="equals" />
                <field name="orderChannel" />
                <field name="poType" />
                <field name="IndirectCustomer" />
                <field name="unit" />
            </field-list>
        </advanced-search>
    </context>

    <context type="JnJProduct" component="advanced-search" merge-by="type" parent="Product">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="catalogId" />
            </field-list>
        </advanced-search>
    </context>

    <context type="JnJLaProduct" component="advanced-search" merge-by="type" parent="JnJProduct">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="sector" />
                <field name="ecommerceFlag" />
                <field name="productStatusCode" />
            </field-list>
        </advanced-search>
    </context>

    <context type="JnjDropShipmentDetails" component="advanced-search" merge-by="type">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="salesOrganization" />
            </field-list>
        </advanced-search>
    </context>

    <context type="jnjInvoiceOrder" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="invDocNo" selected="true" operator="equals" />
                <field name="docNumber" />
                <field name="nfNumber" />
            </field-list>
        </advanced-search>
    </context>

    <context type="JnjContract" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="eCCContractNum" />
                <field name="status" />
                <field name="unit" />
                <field name="startDate" />
                <field name="endDate" />
                <field name="accounts" selected="true" />
            </field-list>
        </advanced-search>
    </context>

    <context type="JnjUploadOrderSHA" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="xmlFileStatus" />
                <field name="userFileName" />
                <field name="xmlFileName" />
            </field-list>
        </advanced-search>
    </context>

    <context type="JnjDropShipmentDetails" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="id" selected="true" />
                <field name="materialId" selected="true" />
            </field-list>
        </advanced-search>
    </context>

    <context type="LoadTranslation" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="b2bUnit" selected="true" />
                <field name="productId" selected="true" />
            </field-list>
        </advanced-search>
    </context>

    <context type="JnjOrdEntStsMapping" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="GTSHold" />
                <field name="backorderStatus" />
                <field name="carrierActualDeliveryDateStatus" />
                <field name="carrierEstDeliveryDateStatus" />
                <field name="deliveryStatus" />
                <field name="invoiceStatus" />
                <field name="overAllStatus" />
                <field name="rejectionStatus" />
                <field name="orderEntryStatus" />
            </field-list>
        </advanced-search>
    </context>

    <context type="B2BUnit" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="name" selected="true" operator="contains" />
                <field name="uid" selected="true" operator="equals" />
                <field name="country" />
            </field-list>
        </advanced-search>
    </context>

    <context type="JnjConfig" component="advanced-search" merge-by="type" parent="GenericItem">
        <advanced-search xmlns="http://www.hybris.com/cockpitng/config/advancedsearch">
            <field-list>
                <field name="id" selected="true" operator="equals" />
                <field name="key" selected="false" operator="contains" />
                <field name="description" selected="false" operator="contains" />
                <field name="value" selected="false" operator="contains" />
            </field-list>
        </advanced-search>
    </context>


</config>
