<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="jnjlabackofficeService" class="com.jnj.la.backoffice.services.JnjlabackofficeService" />

	<bean id="jnjlabackofficeLabelLocator" class="com.hybris.cockpitng.util.labels.ResourcesLabelLocator" scope="singleton" init-method="init" lazy-init="false">
  		<property name="location" value="/jnjlabackoffice-backoffice-labels/"/>
  		<property name="name" value="labels"/>
  	</bean>

	<alias name="extensionOneLoginInformationHandler" alias="loginInformationHandler"/>
	<bean id="extensionOneLoginInformationHandler" parent="defaultLoginInformationHandler" init-method="init" scope="session">
		<property name="configuration">
			<list merge="false">
				<bean class="com.hybris.cockpitng.handler.login.LoginInformationConfigData">
					<constructor-arg value="tenant"></constructor-arg>
					<constructor-arg value=" "></constructor-arg>
					<constructor-arg>
						<value type="java.lang.Integer">0</value>
					</constructor-arg>
				</bean>
			</list>
		</property>
	</bean>
</beans>
