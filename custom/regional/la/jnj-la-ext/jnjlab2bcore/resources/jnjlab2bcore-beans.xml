<?xml version="1.0" encoding="ISO-8859-1"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="com.jnj.la.core.data.JnJLaInvoiceRowError">
		<property name="invoice" type="java.lang.String"/>
		<property name="estDeliveryDate" type="java.lang.String"/>
		<property name="actualDeliveryDate" type="java.lang.String"/>
		<property name="errorMessage" type="java.lang.String"/>
	</bean>

	<bean class="com.jnj.la.core.data.order.JnjLAEmailScheduleLineData">
		<property name="expectedDeliveryDate" type="java.lang.String" />
		<property name="quantity" type="java.lang.String" />
		<property name="scheduledLineNumber" type="java.lang.String" />
	</bean>

	<bean class="com.jnj.la.core.data.order.JnjLAEmailOrderEntryData">
		<property name="productCode" type="java.lang.String" />
		<property name="productName" type="java.lang.String" />
		<property name="orderedQuantity" type="java.lang.String" />
		<property name="invoicedQuantity" type="java.lang.String" />
		<property name="openedQuantity" type="java.lang.String" />
        <property name="price" type="java.lang.String" />
        <property name="requestedDeliveryDate" type="java.lang.String" />
		<property name="scheduleLines" type="java.util.List&lt;com.jnj.la.core.data.order.JnjLAEmailScheduleLineData>" />
        <property name="status" type="java.lang.String" />
	</bean>

    <bean class="com.jnj.la.core.data.order.JnjLAEmailOrderData">
		<property name="code" type="java.lang.String" />
        <property name="sapOrderNumber" type="java.lang.String" />
        <property name="customerOrderNumber" type="java.lang.String" />
        <property name="shipToName" type="java.lang.String" />
        <property name="shipToAddress" type="java.lang.String" />
        <property name="orderTypeDescription" type="java.lang.String" />
        <property name="orderEntries" type="java.util.List&lt;com.jnj.la.core.data.order.JnjLAEmailOrderEntryData>" />
        <property name="status" type="java.lang.String" />
		<property name="customer" type="java.lang.String" />
		<property name="nextDeliveryDate" type="java.lang.String" />
		<property name="deliveryDate" type="java.lang.String" />
    </bean>

	<bean class="com.jnj.facades.data.JnJInvoiceOrderData">
		<property name="carrierEstimateDeliveryDate" type="java.util.Date" />
		<property name="carrierConfirmedDeliveryDate" type="java.util.Date" />
	</bean>

    <bean class="com.jnj.la.core.data.order.JnjLAConsolidatedEmailRowData">
        <property name="firstHierarchy" type="java.lang.String" />
        <property name="openedSum" type="java.lang.String" />
    </bean>

	<bean class="com.jnj.la.core.data.order.JnjLAConsolidatedReportRowData">
		<property name="sapOrderNumber" type="java.lang.String" />
		<property name="customerOrderNumber" type="java.lang.String" />
		<property name="orderType" type="java.lang.String" />
		<property name="issuerCode" type="java.lang.String" />
		<property name="issuerName" type="java.lang.String" />
		<property name="deliveryAddress" type="java.lang.String" />
		<property name="creationDate" type="java.util.Date" />
		<property name="salesOrderChannel" type="java.lang.String" />
		<property name="firstHierarchy" type="java.lang.String" />
		<property name="productCode" type="java.lang.String" />
		<property name="productName" type="java.lang.String" />
		<property name="customerMaterialCode" type="java.lang.String" />
		<property name="quantity" type="java.lang.String" />
		<property name="invoicedQuantity" type="java.lang.String" />
		<property name="openedQuantity" type="java.lang.String" />
		<property name="salesUnit" type="java.lang.String" />
        <property name="unitPrice" type="java.lang.Double" />
		<property name="unitPriceString" type="java.lang.String" />
		<property name="sapLineNumber" type="java.lang.String" />
