		<property name="estimatedDeliveryDate" type="java.util.Date" />
		<property name="orderStatus" type="java.lang.String" />
		<property name="contractReference" type="java.lang.String" />
		<property name="indirectCustomer" type="java.lang.String" />
		<property name="lotNumber" type="java.lang.String" />
		<property name="invoiceNumber" type="java.lang.String" />
		<property name="createdBy" type="java.lang.String" />
		<property name="requestedDeliveryDate" type="java.util.Date" />
        <property name="hasChanged" type="java.lang.Boolean" />
		<property name="eanNumber" type="java.lang.String"  />
		<property name="currency" type="java.lang.String"  />
	</bean>

    <bean class="com.jnj.la.core.data.order.JnjLAConsolidatedEmailData">
        <property name="customer" type="java.lang.String" />
        <property name="reportRows" type="java.util.List&lt;com.jnj.la.core.data.order.JnjLAConsolidatedReportRowData>" />
    </bean>

	<bean class="com.jnj.la.core.dto.JnjLaCronJobMonitoringEmailDto">
		<property name="jobCode" type="String" />
		<property name="country" type="String" />
		<property name="frequency" type="String" />
		<property name="serviceLevelStartTime" type="String" />
		<property name="serviceLevelEndTime" type="String" />
		<property name="monitoringCode" type="String" />
		<property name="monitored" type="boolean" />
		<property name="lastStatus" type="String" />
		<property name="currentStatus" type="String" />
		<property name="actualStartTime" type="String" />
		<property name="actualEndTime" type="String" />
		<property name="averageRunningDuration" type="String" />
		<property name="paused" type="boolean"/>
		<property name="remarks" type="String" />
		<property name="category" type="String" />
		<property name="parentJobName" type="String" />
		<property name="critical" type="String" />
		<property name="deadLine" type="String" />
	</bean>

	<bean class="com.jnj.la.core.data.JnjArchiveOrderData">
		<property name="orderCode" type="String" />
		<property name="deletionStatus" type="Boolean"/>
		<property name="recordsDeleted" type="Integer"/>
		<property name="totalRecords" type="Integer"/>
	</bean>

	<bean class="com.jnj.la.core.data.ReplacementProductData">
		<property name="entryNumber" type="Integer"/>
		<property name="originalProductCode" type="java.lang.String"/>
		<property name="quantity" type="java.lang.Long"/>
		<property name="replacementProductCode" type="java.lang.String"/>
	</bean>
