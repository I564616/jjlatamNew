<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">
           
    <context:annotation-config/> 

	<bean class="org.springframework.beans.factory.annotation.RequiredAnnotationBeanPostProcessor"/>

	<!-- Expose /acceleratoraddon/web/webroot/WEB-INF/messages/base.js.properties  resource bundle as javascript variables -->
	<bean id="defaultJnjlaselloutaddonJavaScriptMessageResourceAccessor" parent="abstractJavaScriptMessageResourceAccessor">
		<property name="addOnName" value="jnjlaselloutaddon"/>
	</bean>

	<bean id="defaultJnjlaselloutaddonBeforeViewJsPropsHandlerAdaptee" parent="abstractBeforeViewJsPropsHandlerAdaptee" class="de.hybris.platform.addonsupport.interceptors.ExportJsPropertiesBeforeViewHandler">
		<property name="messageSource" ref="defaultJnjlaselloutaddonJavaScriptMessageResourceAccessor"/>
	</bean>
	
	
	<bean id="sellOutReportController" class="com.jnj.la.b2b.jnjlaselloutaddon.controllers.pages.JnjSellOutReportController" />
		<alias alias="jnjSellOutReportController" name="sellOutReportController" />

	<bean id="uploadPageController" class="com.jnj.la.b2b.jnjlaselloutaddon.controllers.pages.JnjLatamUploadOrderPageController"/>
	

	<bean id="getJnjlaselloutaddonBeforeViewHandler" parent="beforeViewHandlerAdapter">
		<property name="adaptee">
			<ref bean="defaultJnjlaselloutaddonBeforeViewJsPropsHandlerAdaptee"/>
		</property>
	</bean>
	<bean id="JnjlaselloutaddonBeforeViewHandlerListMergeDirective" depends-on="beforeViewHandlersList" parent="listMergeDirective">
		<property name="add" ref="getJnjlaselloutaddonBeforeViewHandler"/>
	</bean>

</beans>
