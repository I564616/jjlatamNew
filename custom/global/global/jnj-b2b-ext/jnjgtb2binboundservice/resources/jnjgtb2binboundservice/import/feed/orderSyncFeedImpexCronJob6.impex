""SELECT t.CONFIRMED_QTY, t.SCHED_DELIVERY_DT, t.DELIVERY_LINE_BLOCK, t.LINE_STATUS, t.ORD_LINE_NUM, t.ORDER_NUM, t.SCHEDULE_LINE_NUM, t.SCHED_SHIP_DT, t.SOURCE_SYS_ID, t.REC_TIMESTAMP from STG_ORDER_LINE_SCH t, STG_ORDER_CJ cj WHERE t.order_num = cj.order_num and t.REC_TIMESTAMP = cj.REC_TIMESTAMP and t.BUSINESS_SECTOR = cj.BUSINESS_SECTOR"");"
